    <nav id="footer">
        <div class="container">
            {% include footer.html %}
        </div>
    </nav>

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script type="text/javascript" src="{{ ASSET_PATH }}/js/jquery.1.11.1.js"></script>
   
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script type="text/javascript" src="{{ ASSET_PATH }}/js/bootstrap.js"></script>
    <script type="text/javascript" src="{{ ASSET_PATH }}/js/SmoothScroll.js"></script>
    <script type="text/javascript" src="{{ ASSET_PATH }}/js/jquery.isotope.js"></script>
    <script src="{{ ASSET_PATH }}/js/owl.carousel.js"></script>
    

    <!-- Javascripts
    ================================================== -->
    <script type="text/javascript" src="{{ ASSET_PATH }}/js/main.js"></script>
  </body>
</html>